{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "egg": {
    "framework": "egg-cloud"
  },
  "scripts": {
    "start": "npm run init && egg-scripts start --daemon --title=egg-server-{{name}}",
    "stop": "egg-scripts stop --title=egg-server-{{name}}",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "rpc": "egg-rpc-generator",
    "init": "npm run rpc",
    "autod": "autod"
  },
  "dependencies": {
    "egg-scripts": "^2.9.1",
    "egg-cloud": "^0.0.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.9.0",
    "egg-ci": "^1.9.2",
    "egg-mock": "^3.20.1",
    "egg-rpc-generator": "^1.1.0",
    "eslint": "^5.6.1",
    "eslint-config-egg": "^7.1.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "{{author}}",
  "license": "MIT"
}
